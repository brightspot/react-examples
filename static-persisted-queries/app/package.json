{
  "name": "static-persisted-queries",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@types/node": "16.7.13",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "graphql": "16.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "4.4.2"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.yml",
    "mapping": "node updateMapping.mjs",
    "format": "npx prettier --write src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@graphql-codegen/cli": "2.8.1",
    "@graphql-codegen/typed-document-node": "2.3.1",
    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-operations": "2.5.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.1",
    "crypto-hash": "2.0.1",
    "dotenv": "^16.0.3",
    "eslint-config-prettier": "8.5.0",
    "graphql-codegen-persisted-query-ids": "0.1.2",
    "graphql-tag": "2.12.6",
    "prettier": "2.7.1"
  }
}
