{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "brightspot-types/*": ["brightspot-types/*"]
    },
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "es2022",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true
  }
}
